        if ($("#mobile_nav").is(":hidden")) {
            $("#mobile_nav").show().css({
                "left" : -270
            }).animate({
                "left" : 0
            }, 300);
        } else {
            $("#mobile_nav").animate({
                "left" : -270
            }, 300, function() {
                $(this).hide();
            });
        }
        return false;
    });
