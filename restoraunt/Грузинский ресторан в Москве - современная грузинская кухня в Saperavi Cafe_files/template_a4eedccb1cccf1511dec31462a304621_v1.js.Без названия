                    hide_loader();
                    open_new_window("", otvet, function() {
                        $("#new_window").addClass("tovar_detail");
                        $("#new_window select").uniform();
                    });
                });
            }
        } else {
            if (show_loader()) {
                $.post(core, {
                    "act" : "tovar_detail",
                    "id" : id
                }, function(otvet) {
                    hide_loader();
                    open_new_window("", otvet, function() {
                        $("#new_window").addClass("tovar_detail");
                        $("#new_window select").uniform();
                    });
                });
            }
        }
        return false;
    });
