                if ($(this).scrollTop() > 320) {
                    if (getCookie('cookie_wine') == 'Y') {
                        $(".promo-wrap").fadeOut(1000).addClass('hidden');
                    } else {
                        $(".promo-wrap").fadeIn(1000).removeClass('hidden');
                    }
                } else {
                    $(".promo-wrap").fadeOut(1000).addClass('hidden');
                }
            } else {
                if ($(this).scrollTop() > 320) {
                    if (getCookie('cookie_wine') == 'Y') {
                        $(".promo-wrap").fadeOut(1000).addClass('hidden');
                    } else {
                        $(".promo-wrap").fadeIn(1000).removeClass('hidden');
                    }
                } else {
                    $(".promo-wrap").fadeOut(1000).addClass('hidden');
                }
            }
        }
    });
    $('.promo-wrap .close').on('click', function() {
        now = new Date();
        var date = new Date(now.setDate(now.getDate() + 1));
        document.cookie = "cookie_wine=Y ; expires=" + date.toUTCString() + "; path=/;";
        $(".promo-wrap").fadeOut(1000).addClass('hidden');
    });
