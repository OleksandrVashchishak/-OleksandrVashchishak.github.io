        $.uniform.update("#bron-2 .rest_selector select[name=rest]");
        return false;
    });
    $("#bron-2 .rest_selector .ttl .down").click(function() {
        var prev = $("#bron-2 .rest_selector select[name=rest] option:selected").prop("selected", false).prev();
        if (prev.length == 0 || !prev)
            prev = $("#bron-2 .rest_selector select[name=rest] option:last");
        prev.prop("selected", true);
        $.uniform.update("#bron-2 .rest_selector select[name=rest]");
        return false;
    });

