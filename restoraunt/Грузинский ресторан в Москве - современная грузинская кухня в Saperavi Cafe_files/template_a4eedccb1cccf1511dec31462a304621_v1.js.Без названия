                $(this).addClass("opened");
            } else {
                $(this).parent("li").find("ul").slideUp(150);
                $(this).removeClass("opened");
            }
            return false;
        } else if ($("#menu_section_" + $(this).attr("data-section")).length) {
            $("#menu nav ul li ul").slideUp(150);
            $("#menu nav a").removeClass("opened");
            $("#menu nav a").removeClass("active");
            $(this).addClass("active");
            $('html, body').animate({
                scrollTop : $("#menu_section_" + $(this).attr("data-section")).offset().top
            }, 700);
            return false;
        }
    });
    $("#delivery .sections nav ul li a, #dynamic_menu nav ul li a").click(function() {
        if ($("#menu_section_" + $(this).attr("data-section")).length) {
            $("#delivery .sections nav ul li a, #dynamic_menu nav ul li a").removeClass("active");
            $("#dynamic_menu nav ul li a[data-section=" + $(this).attr("data-section") + "]").addClass("active");
            $(this).addClass("active");
            //event.preventDefault();
            $('html, body').animate({
                scrollTop : $("#menu_section_" + $(this).attr("data-section")).offset().top - 140
            }, 700);
            return false;
        }
    });
