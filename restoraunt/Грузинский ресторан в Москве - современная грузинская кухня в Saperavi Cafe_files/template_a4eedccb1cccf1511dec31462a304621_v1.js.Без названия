$(document).ready(function() {
    /*if($.cookie('subscribe_window')!="Y") {
        $.post(core, {"action":"subscribe_window"}, function(otvet){
                open_new_window("", otvet, function(){
                    //$.cookie('subscribe_window', 'Y', { path: '/' });
                });
        },"html");
    }*/
    $("BODY").on("submit", "#subscribe_form form", function(){
        var form = $(this);
        form.ajaxSubmit({
            beforeSubmit: check_form,
            dataType: 'json',
            url: core,
            success: function (otvet) {
                hide_loader();
                if(otvet.STATUS=="ok"){
                    yaCounter35126560.reachGoal('podpiska');
                    console.log(yaCounter35126560);
                    gtag('event', 'podpiska', {
                        'event_category' : 'Form'
                    });
                    form.find(".submit").hide();
                    form.find(".otvet").show().html(otvet.TEXT);
                    hide_new_window();
                }
            }
        });
        return false;
     });
    $("BODY").on("submit", "#subscribe_window", function(){
        var form = $(this);
        form.ajaxSubmit({
            beforeSubmit: check_form,
            dataType: 'json',
            url: core,
            success: function (otvet) {
                hide_loader();
                if(otvet.STATUS=="ok"){
                    yaCounter35126560.reachGoal('podpiska_popup');
                    console.log(yaCounter35126560);
                    gtag('event', 'podpiska', {
                        'event_category' : 'Form'
                    });
                    form.find(".submit").hide();
                    form.find(".otvet").show().html(otvet.TEXT);
                    hide_new_window();
                }
            }
        });
        return false;
     });
});
