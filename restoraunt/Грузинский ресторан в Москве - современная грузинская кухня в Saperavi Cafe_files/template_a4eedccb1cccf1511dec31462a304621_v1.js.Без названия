function check_form(formData, form) {
    var no_errors = true;
    var to_send = {};
    form.find(".error").removeClass("error");
    form.find(".error_text").remove();
    form.find("input[type=checkbox],input[type=text],input[type=hidden],input[type=radio]:checked, select, textarea").each(function () {
        if($(this).prop("disabled"))
            return;
        
        if($(this).attr('name') == 'email' && !validateEmail($(this).val())) {
            no_errors = false;
            $(this).addClass("error");
            if ($(this).data("error") != "" && $(this).data("error"))
                if ($(this).parent(".ln").find(".error_text").length == 0)
                    $(this).after('<span class="error_text" title="' + $(this).data("error") + '"></span>');
        }
        if ($(this).hasClass("ob") && $(this).is(":visible") && ($(this).val() == "" || ($(this).attr("type") == "checkbox" && !$(this).is(":checked")))) {
            no_errors = false;
            $(this).addClass("error");
            if ($(this).data("error") != "" && $(this).data("error"))
                if ($(this).parent(".ln").find(".error_text").length == 0)
                    $(this).after('<span class="error_text" title="' + $(this).data("error") + '"></span>');
        }
        to_send[$(this).attr("name")] = $(this).val();
    });
    if (no_errors) {
        show_loader();
        console.log(no_errors);
        return to_send;
    } else {
        return false;
    }
}
function validateEmail(email) {
    var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
    return re.test(email);
}
$(document).ready(function () {
    $("#bron-2").submit(function () {
        var form = $(this);
        form.ajaxSubmit({
            beforeSubmit: check_form,
            dataType: 'json',
            url: core,
            success: function (otvet) {
                hide_loader();
                form.find(".otvet").html(otvet.TEXT);
                form.find(".submit").slideUp(200);
                yaCounter35126560.reachGoal('reserve');
                gtag('event', 'reserve', {
                    'event_category' : 'Form'
                });
            }
        });
        return false;
    });
    $("#contacts_form").submit(function () {
        var form = $(this);
        form.ajaxSubmit({
            beforeSubmit: check_form,
            dataType: 'json',
            url: core,
            success: function (otvet) {
                hide_loader();
                form.find(".otvet").html(otvet.TEXT);
                form.find(".submit").slideUp(200);
                yaCounter35126560.reachGoal('feedback');
                gtag('event', 'feedback', {
                    'event_category' : 'Form'
                });
            }
        });
        return false;
    });
    $("#bron-2").on("click", ".date_select label", function () {
        $(".date_select label").removeClass("selected");
        $(this).addClass("selected");
        return false;
    });
    if ($('.time').length > 0) {
        var time_options = {
            onKeyPress: function (cep, e, field, options) {
                if (cep[0] == 2) {
                    $('.time').mask('23:50', time_options);
                } else {
                    $('.time').mask('20:50', time_options);
                }
            },
            'translation': {
                2: {pattern: /[0-2]/},
                3: {pattern: /[0-3]/},
                5: {pattern: /[0-5]/}
            }
        };
        $('.time').mask('20:50', time_options);
    }
    $('form input[name=tel],form input[name=telephone]').mask('+7 (000) 000-00-00');
});
