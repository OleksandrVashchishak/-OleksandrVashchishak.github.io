; /* Start:"a:4:{s:4:"full";s:56:"/local/templates/Saperavi_NEW/js/cart.js?159231292219094";s:6:"source";s:40:"/local/templates/Saperavi_NEW/js/cart.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
function add2cart_animation(obj) {
    var img = obj.find(".photo").clone();
    $("#container").append(img);
    if ($(".mobile_cart").is(":hidden")) {
        if ($("#rcart").length > 0)
            var ct = $("#rcart").offset().top;
        else
            var ct = $("#top_cart").offset().top;
        if ($("#rcart").length > 0)
            var cl = $("#rcart").offset().left;
        else
            var cl = $("#top_cart").offset().left;
    } else {
        var cl = $(".mobile_cart").offset().left;
        var ct = $(".mobile_cart").offset().top;
    }
    img.css({
        "position" : "absolute",
        "z-index" : 9999,
        "opacity" : 0.9,
        "left" : obj.find(".photo").offset().left,
        top : obj.find(".photo").offset().top,
        width : obj.find(".photo").width(),
        height : obj.find(".photo").width(),
    }).animate({
        top : ct,
        left : cl,
        width : 50,
        rotate : "+=360",
        opacity : 0.1
    }, 500, function() {
        img.remove();
    });
}

function check_order() {
    if ($("#rcart .in").data("can_order") == "Y")
        $("#order_form .submit").replaceWith('<input type="submit" name="submit" value="Оформить заказ" class="submit new_order" />');
    else
        $("#order_form .submit").replaceWith('<input type="submit" name="submit" value="Оформить заказ" class="submit new_order disabled" uk-tooltip="title: Минимальная стоимость заказа - 1500 руб.; pos: top-center"/>');
}

function add_input(item, co, id) {
    var input = '<div class="inp"><div class="plus_but" data-product-id="' + id + '"><span class="plus" ></span></div><div class="minus_but" data-product-id="' + id + '"><span class="minus" ></span></div><div class="in_cart">В корзине: <span>' + co + '</span></div></div>';
    item.find(".but_place").html(input);
}

function restore_button() {    
    var $button = $(this).closest(".tovar").find(".but_place");
    var id =  $(this).closest(".tovar")[0].id.replace("menu_product_", "");
    var input = '<a href="#' + id + '" data-product-id="' + id + '" class="add2cart"><span class="cart"></span>В корзину</a>';
    
    $button.html(input);
}
function in_cart_items() {
    $("#rcart .list .item").each(function() {
        var id = $(this).data("item_id");
        var co = $(this).find("b").text();
        if (!$("#menu_product_" + id).hasClass('vino')) {
            add_input($("#menu_product_" + id), co, $(this).data("cart_id"));
        }
    });
}

var map,
    marker,
    myCollection;

function setMarker(lat, lon) {
    myCollection.removeAll();
    marker = new ymaps.Placemark([lat, lon], {}, {
        balloonCloseButton : true,
        iconLayout : 'default#image',
        iconImageHref : '/local/templates/Saperavi_NEW/i/map_marker.png',
        iconImageSize : [45, 43],
        iconImageOffset : [-17, -43]
    });
    myCollection.add(marker);
    map.geoObjects.add(myCollection);
}

function initMap() {
    map = new ymaps.Map("map", {
        center : [55.753058, 37.617269],
        zoom : 16
    });
    myCollection = new ymaps.GeoObjectCollection();
    setMarker(55.753058, 37.617269)
}

function address_window_init() {
    if ($(window).width() < 520) {
        $("#address_select").css({
            "width" : $(window).width() - 30
        });
        $("#new_window .close_window").hide();
    }
    $("#addr_input").suggestions({
        token : "12fef456f7564210d59b9bb8cd5b3a245aeae214",
        type : "ADDRESS",
        autoSelectFirst : true,
        count : 5,
        minChars : 3,
        /* Вызывается, когда пользователь выбирает одну из подсказок */
        onSelect : function(suggestion) {
            console.log(suggestion);
        },
        constraints : {
            locations : {
                region : "Москва"
            },
            deletable : false
        },
        restrict_value : true,
        onSelect : function(suggestion) {
            errors = '';
            if (suggestion.data.geo_lat && suggestion.data.geo_lon) {
                console.log(suggestion.data.house_type);
                if (suggestion.data.street_type == null) {
                    $("#addr_input").suggestions().setSuggestion({
                        "value" : $("#addr_input").val() + " ул. ",
                        "data" : suggestion.data
                    });
                } else {
                    if (suggestion.data.house_type == null) {
                        $("#addr_input").suggestions().setSuggestion({
                            "value" : $("#addr_input").val() + " д. ",
                            "data" : suggestion.data
                        });
                    } else {
                        if (suggestion.data.flat_type == null) {
                            $("#addr_input").suggestions().setSuggestion({
                                "value" : $("#addr_input").val() + " кв. ",
                                "data" : suggestion.data
                            });
                        }
                    }
                }
                map.setCenter([suggestion.data.geo_lat, suggestion.data.geo_lon], 16, {
                    checkZoomRange : true
                });
                setMarker(suggestion.data.geo_lat, suggestion.data.geo_lon);
            }
        }
    });
    ymaps.ready(initMap);
}

$(document).ready(function() {
    $('.sumoselect[name="nap"]').on("change", restore_button);
    in_cart_items();
    check_order();
    $('body')
        .on("click", ".add2cart", function() {
            var $addBtn = $(this);

            if (!$addBtn.hasClass('wine'))
            {
                var to_send = {};

                to_send["id"] = id = $addBtn.attr("data-product-id");
                to_send["act"] = "add2cart";
                to_send["Q"] = 1;

                // варианты из списка
                var $nap = $("#nap_" + to_send["id"]);
                if ($nap.length > 0) {
                    to_send["nap"] = $nap.val();
                    
                    if (!to_send["nap"]) {
                        $nap.siblings('.SelectBox').click();
                        return false;
                    }
                }

                var tovar = $addBtn.closest(".product");

                add2cart_animation(tovar);
                add_input(tovar, 1, id);

                if (show_loader())
                {
                    $.post(core, to_send, function(otvet) {
                        $("#top_cart .sod").html(otvet.TOP_CART);
                        $(".mobile_cart .co").html(otvet.CO);

                        add_input(tovar, 1, otvet.CART_ID);
                        hide_loader();

                        $("#rcart").load("/local/include_areas/aside_cart.php", function() {
                            check_order();
                            $('#rcart .list').jScrollPane({
                                autoReinitialise : true
                            });
                        });

                        if (!$addBtn.hasClass('no-recommend'))
                        {
                            var to_send = {
                                'id': id,
                                'act': 'recommend'
                            };

                            $.post(core, to_send, function(html) {
                                if (!html) { return; }

                                open_new_window('Рекомендуемые', html, function(){
                                    $("#new_window").removeClass("tovar_detail");

                                    $('#recomendations .owl-carousel').owlCarousel({
                                        loop: false,
                                        nav: true,
                                        margin: 10,
                                        dots: false,
                                        navText: [
                                            '<i class="fa fa-angle-left" aria-hidden="true"></i>',
                                            '<i class="fa fa-angle-right" aria-hidden="true"></i>'
                                        ],
                                        responsive: {
                                            0: {
                                                items: 1
                                            },
                                            700: {
                                                items: 2
                                            },
                                            1000: {
                                                items: 3
                                            },
                                            1200: {
                                                items: 4
                                            }
                                        }
                                    });

                                    $('.continue').on('click', function(e){
                                        e.preventDefault();
                                        $(this).closest('.modal-dialog').find('.close_window').click();
                                    });
                                });
                            });
                        }
                    }, "json");
                }
            }
        })
        .on("click", ".plus_but", function() {
            var to_send = {};
            to_send["id"] = id = $(this).attr("data-product-id");
            to_send["act"] = "change_co";
            var tovar = $(this).closest(".product");
            to_send["co"] = parseInt(tovar.find(".in_cart span").text());
            to_send["co"]++;
            tovar.find(".in_cart span").html(to_send["co"]);
            if (show_loader()) {
                $.post(core, to_send, function(otvet) {
                    $("#top_cart .sod").html(otvet.TOP_CART);
                    $(".mobile_cart .co").html(otvet.CO);
                    hide_loader();
                    $("#rcart").load("/local/include_areas/aside_cart.php", function() {
                        check_order();
                        $('#rcart .list').jScrollPane({
                            autoReinitialise : true
                        });
                    });
                }, "json");
            }
        })
        .on("click", ".minus_but", function() {
            var to_send = {};
            to_send["id"] = id = $(this).attr("data-product-id");
            to_send["act"] = "change_co";
            var tovar = $(this).closest(".product");
            to_send["co"] = parseInt(tovar.find(".in_cart span").text());
            to_send["co"]--;
            if (to_send["co"] < 1)
                to_send["co"] = 1;
            tovar.find(".in_cart span").html(to_send["co"]);
            if (show_loader()) {
                $.post(core, to_send, function(otvet) {
                    $("#top_cart .sod").html(otvet.TOP_CART);
                    $(".mobile_cart .co").html(otvet.CO);
                    hide_loader();
                    $("#rcart").load("/local/include_areas/aside_cart.php", function() {
                        check_order();
                        $('#rcart .list').jScrollPane({
                            autoReinitialise : true
                        });
                    });
                }, "json");
            }
        })
        .on("click", ".new_order.disabled", function() {
            return false;
        })
        .on("click", "#rcart .del_item", function() {
            var to_send = {};
            to_send["id"] = $(this).data("item_id");
            to_send["act"] = "del_item";
            var obj = $(this).closest(".item");
            if (show_loader()) {
                $.post(core, to_send, function(otvet) {
                    obj.remove();
                    hide_loader();
                    if (otvet.CO < 1 || !otvet.CO)
                        window.location.replace("/delivery/");
                    else {
                        $("#rcart .itog span").html(otvet.SUMM);
                        $("#top_cart .sod").html(otvet.TOP_CART);
                        if (parseInt(otvet.SUMM2) < 1500)
                            $("#order_form .submit").addClass("disabled");
                        else
                            $("#order_form .submit").removeClass("disabled");
                    }
                }, "json");
            }
            return false;
        })
        .on("submit", "#order_form", function() {
            var form = $(this);
            form.ajaxSubmit({
                beforeSubmit : check_form,
                dataType : 'json',
                url : core,
                success : function(otvet) {
                    console.log(otvet);
                    hide_loader();
                    if (otvet.STATUS == "ok") {
                        yaCounter35126560.reachGoal('order');
                        console.log(yaCounter35126560);
                        window.location.replace('/delivery/final.php?ORDER_ID=' + otvet.ORDER_ID + '&HASH=' + otvet.HASH);
                        $("#order_confirm .otvet").slideUp(200);
                    } else if (otvet.STATUS == "error") {
                        $("#order_confirm .otvet").html(otvet.TEXT).slideDown(200);
                    }
                    //open_new_window("", otvet, function() {});
                }
            });

            return false;
        })
        .on("click", "#order_confirm .check_sms_code", function() {
            var sms_code = $("#order_confirm input[name=sms_code]").val();
            $("#order_confirm .error").removeClass("error");
            var error = false;
            if (!$("#agree").is(":checked")) {
                error = true;
                $("#agree").addClass("error");
            }
            if ($("#order_confirm .pole").val() == "") {
                error = true;
                $("#order_confirm .pole").addClass("error");
            }
            if (!error) {
                var form = $("#order_form");
                form.ajaxSubmit({
                    beforeSubmit : check_form,
                    dataType : 'json',
                    data : {
                        "sms_code" : sms_code
                    },
                    url : core,
                    success : function(otvet) {
                        hide_loader();
                        if (otvet.STATUS == "ok") {
                            yaCounter35126560.reachGoal('order');
                            console.log(yaCounter35126560);
                            gtag('event', 'order', {
                                'event_category' : 'Form'
                            });
                            window.location.replace('/delivery/final.php?ORDER_ID=' + otvet.ORDER_ID + '&HASH=' + otvet.HASH);
                            $("#order_confirm .otvet").slideUp(200);
                        }
                        if (otvet.STATUS == "error") {
                            $("#order_confirm .otvet").html(otvet.TEXT).slideDown(200);
                        }
                    }
                });
            }
            return false;
        })
        .on("focus", "#order_form input[name=address]", function() {
            if ($(this).val() == "") {
                if (show_loader()) {
                    $.post(core, {
                        "act" : "address_window"
                    }, function(otvet) {
                        hide_loader();
                        open_new_window("", otvet, function() {
                            address_window_init();
                        });
                    }, "html");
                }
            }
        })
        .on("click", "#address_select .ok_but", function() {
            var addr = $("#addr_input").val();
            $("#order_form input[name=address]").val(addr);
            if (addr != "")
                $("#order_form .chage_addr").show();
            $.cookie("address", addr, {
                path : '/'
            });
            hide_new_window();
            return false;
        })
        .on("click", "#order_form .chage_addr a", function() {
            if (show_loader()) {
                $.post(core, {
                    "act" : "address_window"
                }, function(otvet) {
                    hide_loader();
                    open_new_window("", otvet, function() {
                        address_window_init();
                    });
                }, "html");
            }
            return false;
        })
        .on("click", "#address_select .back_link", function() {
            hide_new_window();
            return false;
        });

    $('input[name=time_type]').change(function() {
        if ($(this).val() == "t1") {
            $(".time_type_t2").addClass("disabled");
        } else {
            $(".time_type_t2").removeClass("disabled");
            $("#datepicker2").datepicker();
        }
    });
    

    $('input[name=payment_type]').change(function() {
        if ($(this).val() == "1") {
            $(".kupura_block").removeClass("disabled");
            $(".kupura_block input").prop("disabled", false);
        } else {
            $(".kupura_block").addClass("disabled");
            $(".kupura_block input").prop("disabled", true);
        }
    });
    
    $('#rcart .list').jScrollPane({
        autoReinitialise : true
    });
    /*$("#order_form input[type=text]").change(function() {
        var name = $(this).attr("name");
        var val = $(this).val();
        $.cookie(name, val, {
            path : '/'
        });
    });*/
    $("#order_form").on("click", ".delivery_select label", function() {
        var delivery = $(this).data("id");
        $("#order_form input[name=delivery]").val(delivery);
        $(".delivery_select label").removeClass("selected");
        $(this).addClass("selected");
        if (delivery == 4) {
            //самовывоз
            $("#order_form input[name=address], #order_form input[name=metro], .time_block, .kupura_block").hide();
            $("#order_form .delivery_type_d2").show();
        } else {
            $("#order_form input[name=address], #order_form input[name=metro], .time_block, .kupura_block").show();
            $("#order_form .delivery_type_d2").hide();
        }
        show_loader();
        $.get(core, {
            "act" : "recalc_order",
            "delivery" : delivery
        }, function(otvet) {
            hide_loader();
            if (otvet.DISCOUNT && otvet.DISCOUNT == "Y") {
                $("#order_form .itog_discount").show();
                $("#order_form .itog_discount span").html(otvet.ORDER_PRICE);
            } else {
                $("#order_form .itog_discount").hide();
            }
        }, "json");
        return false;
    });
    $(".tovar .name a").click(function() {
        return false;
    });
});
/* End */
