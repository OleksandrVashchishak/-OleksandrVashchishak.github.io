var core = "/core/core.php";
var AJAX_RUNING = false;
function show_loader() {
    if (AJAX_RUNING)
        return false;
    else {
        AJAX_RUNING = true;
        $("#small_loader").show();
        return true;
    }
}

function hide_loader() {
    AJAX_RUNING = false;
    $("#small_loader").hide();
}

function open_new_window(TITLE, HTML, after) {
    //console.dir('start open');

    $("#new_window .close_window").show();

    if (TITLE != "" && TITLE) {
        $("#new_window .modal-header h2").html(TITLE).show();
    } else {
        $("#new_window .modal-header h2").hide();
    }

    //console.log(HTML);

    $("#new_window .c").html(HTML);
    $("#new_window").addClass("in");
    $("body").addClass("modal-open");

    $("#new_window").show().css({
        "opacity" : 0
    }).animate({
        "opacity" : 1,
    }, 300);

    //console.dir('end open');

    if (typeof after === "function") {
        after();
    }

    //console.dir('opened');
}

function hide_new_window() {
    //console.dir('start close');
    $("body").removeClass("modal-open");
    $("body").removeClass("with-window1");
    $("body").removeClass("with-window0");
    $("#new_window").fadeOut(200);
    $("#new_window").removeClass("tovar_detail");
    //console.dir('end close');
}

var a = false;
function show_abs_menu(scroll) {
    if (scroll > 450) {
        $("#dynamic_menu").css({
            "left" : $("#delivery .fl").offset().left
        }).fadeIn(200);
    } else {
        $("#dynamic_menu").fadeOut(200);
    }
    $("#delivery .section_name").each(function() {
        if ($(this).offset().top <= scroll + 180) {
            var id = $(this).attr("id").replace("menu_section_", "");
            $("#delivery .sections nav ul li a, #dynamic_menu nav ul li a").removeClass("active");
            $("#dynamic_menu nav ul li a[data-section=" + id + "]").addClass("active");
        } else {
        }
    });
}

function getBodyScrollTop() {
    return self.pageYOffset || (document.documentElement && document.documentElement.scrollTop) || (document.body && document.body.scrollTop);
}


$(window).scroll(function() {
    if ($(window).scrollTop() > 80 && $(window).width() < 640) {
        $('.mobile_nav_bg').css('opacity', '1');
        $('header .tel').addClass('white');
    } else {
        $('.mobile_nav_bg').css('opacity', '0');
        $('header .tel').removeClass('white');
    }
})
$(document).ready(function() {
    if (window.location.hash != "") {
        var id = window.location.hash;
        id = id.replace("#", "");
        if ($("#menu_product_" + id).length > 0) {
            $('html, body').animate({
                scrollTop : $("#menu_product_" + id).offset().top
            }, 700, function() {
                $("#menu_product_" + id + " .photo").click();
            });
        }
    }
    if ($("#dynamic_menu").length > 0 && $(window).width() > 520) {
        window.onscroll = function() {
            var scroll = getBodyScrollTop();
            show_abs_menu(scroll);
        }
    }
    if ($(window).width() > 520) {
        setTimeout(function() {
            $("#menu .pinned").pin({
                containerSelector : "#menu"
            });
            $("#delivery .pinned").pin({
                containerSelector : "#delivery"
            });
        }, 1000);
    }
    if ($(".mobile_nav").is(":visible") && $(".mobile_nav").length > 0) {
        $("footer").css({
            "padding-bottom" : 100
        });
    }
    function getCookie(name) {
        var matches = document.cookie.match(new RegExp("(?:^|; )" + name.replace(/([\.$?*|{}\(\)\[\]\\\/\+^])/g, '\\$1') + "=([^;]*)"));
        return matches ? decodeURIComponent(matches[1]) : undefined;
    }


